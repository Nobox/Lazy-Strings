{
    "name": "nobox/lazy-strings",
    "description": "Create localized strings from a Google Docs Spreadsheet.",
    "license" : "MIT",
    "keywords": ["googledocs", "spreadsheet"],
    "homepage": "https://github.com/Nobox/Lazy-Strings",
    "authors": [
        {
            "name": "Alberto Estrada",
            "email": "alberto@nobox.com"
        },
        {
            "name": "Antonio Rodriguez",
            "email": "antonio@nobox.com"
        },
        {
            "name": "Jonathan Torres",
            "email": "jonathan@nobox.com"
        },
        {
            "name": "Waldemar Figueroa",
            "email": "waldemar@nobox.com"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "autoload": {
        "psr-4": {
            "Nobox\\LazyStrings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nobox\\LazyStrings\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "platform": {
            "php": "5.4"
        }
    },
    "minimum-stability": "stable"
}
